{
  "name": "Calo",
  "version": "0.0.1",
  "production": false,
  "enableHooks": [
    "action",
    "filter",
    "shortcode"
  ],
  "description": "a Simple ticketing system WordPress plugin.",
  "default": {
    "calo_animation": {
      "enable": 1,
      "elements": {
        "logo": "bounceIn",
        "tab": "heartBeat",
        "content": "fadeIn",
        "calo_active": "fadeIn",
        "calo_inactive": "fadeOut"
      }
    },
    "calo_assets": {
      "backend": {
        "calo-fontawesome-css": {
          "status": true,
          "type": "css",
          "src": "vendor/fontawesome/css/all.min.css"
        }
      },
      "frontend": {
        "calo-fontawesome-css": {
          "status": true,
          "type": "css",
          "src": "vendor/fontawesome/css/all.min.css"
        }
      }
    },
    "calo_hooks": []
  }
}
